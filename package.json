{
  "name": "@cube-web3/cube3-integration",
  "version": "0.0.1",
  "description": "CUBE3 integration smart contracts",
  "repository": {
    "type": "git",
    "url": "git@github.com:cube-web3/cube3-integration.git"
  },
  "homepage": "https://cube3.ai",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "compile": "hardhat compile && hardhat typechain",
    "test": "hardhat test",
    "coverage: hardhat": "hardhat coverage",
    "prettier": "npx prettier -w .",
    "build:ts": "tsc -b",
    "husky:pre-commit": "npm run prettier && git add .",
    "slither": "slither . --filter-paths \"lib|test|node_modules\"",
    "docs:host": "forge doc --serve --port 4000",
    "coverage:foundry": "forge coverage --report lcov && genhtml lcov.info --branch-coverage --output-dir coverage"
  },
  "keywords": [
    "hardhat",
    "solidity",
    "web3",
    "cube3",
    "security",
    "rasp"
  ],
  "author": "CUBE3.ai",
  "license": "MIT",
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@openzeppelin/hardhat-upgrades": "^2.0.2",
    "@typechain/ethers-v6": "^0.4.3",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^5.2.0",
    "hardhat": "^2.17.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.1",
    "prettier-plugin-solidity": "^1.0.0",
    "solhint": "^3.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.2",
    "@openzeppelin/contracts-upgradeable": "^4.8.2"
  }
}
